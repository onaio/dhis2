<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.hisp.dhis.config.Configuration_IN" table="config_in">
    
	<id name="id" column="configid">
      <generator class="native"/>
    </id>

    <property name="key" column="ckey" not-null="true" unique="true" length="160"/>
    
    <property name="value" column="cvalue" not-null="true" length="160"/>
	  	
  </class>
</hibernate-mapping>
