 <!-- Home Page -->	 
  <bean id="org.hisp.dhis.den.action.EmptyAction"
    class="org.hisp.dhis.den.action.EmptyAction"/>
  
  <bean id="org.hisp.dhis.den.api.LLDataValueStore"
    class="org.hisp.dhis.den.impl.HibernateLLDataValueStore">
