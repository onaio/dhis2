	var financialMonthNames = new Array("Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec","Jan","Feb","Mar");
	
    k = 0;

    for( i = 0; i< monthDays.length; i++ )
    {
        var curMonthDays = monthDays[i];
     
        for( j = 1; j<=curMonthDays; j++ )
        {
            if( (i+1) <= 9 )
            {
               days[k]=  "0"+ (i+1);
            }
            else
            {
	            days[k]=  i+1;
            }
            if( j <= 9 )
            {
                days[k] += "-0"+j;
            }
            else
            {
	            days[k] +=  "-"+j;
            }    
        
            k++;
		}
    }

